{
  "grammars": [
    {
      "name": "jan",
      "camelcase": "Jan",
      "scope": "source.jan",
      "path": ".",
      "file-types": ["jan"],
      "highlights": [
        "queries/highlights.scm"
      ],
      "theme": {
        "comment": "brown",
        "keyword": "red",
        "number": "blue",
        "label": "purple",
        "text": "yellow"
      }
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Parser for my language JAN",
    "authors": [
      {
        "name": "Jan Pedryc",
        "email": "jan.pedryc@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-jan"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true
  }
}
